{
    "eslint.enable": true,
    "eslint.format.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "highlight.regexes": {
        "(?<=ThemeColor.)(Comments)":    { "decorations": [{ "backgroundColor": "#505050",   "color": "white", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(RegExp)":      { "decorations": [{ "backgroundColor": "#d16969",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Strings)":     { "decorations": [{ "backgroundColor": "#ce9178",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Functions)":   { "decorations": [{ "backgroundColor": "#dcdcaa",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Chars)":       { "decorations": [{ "backgroundColor": "#d7ba7d",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Numbers)":     { "decorations": [{ "backgroundColor": "#b5cea8",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(DocComments)": { "decorations": [{ "backgroundColor": "#608b4e",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Types)":       { "decorations": [{ "backgroundColor": "#4ec9b0",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Variables)":   { "decorations": [{ "backgroundColor": "#9cdcfe",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Keywords)":    { "decorations": [{ "backgroundColor": "#569cd6",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(FlowControl)": { "decorations": [{ "backgroundColor": "#c586c0",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Tags)":        { "decorations": [{ "backgroundColor": "#808080",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Noise)":       { "decorations": [{ "backgroundColor": "#404040",   "color": "white", "fontStyle": "italic" }] },

        "(?<=ThemeColor.)(Primary)":     { "decorations": [{ "backgroundColor": "#608b4e",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Secondary)":   { "decorations": [{ "backgroundColor": "#007acc",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Foreground)":  { "decorations": [{ "backgroundColor": "#d4d4d4",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Background)":  { "decorations": [{ "backgroundColor": "#1e1e1e",   "color": "white", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Selection)":   { "decorations": [{ "backgroundColor": "#264f78",   "color": "white", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Error)":       { "decorations": [{ "backgroundColor": "#d16969",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Warning)":     { "decorations": [{ "backgroundColor": "#d7ba7d",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Info)":        { "decorations": [{ "backgroundColor": "#9cdcfe",   "color": "black", "fontStyle": "italic" }] },
        "(?<=ThemeColor.)(Success)":     { "decorations": [{ "backgroundColor": "#608b4e",   "color": "black", "fontStyle": "italic" }] },
    },
    "cSpell.words": [
        "protobuf",
        "vcard",
        "vetur",
        "viml",
        "vuejs"
    ],
}
